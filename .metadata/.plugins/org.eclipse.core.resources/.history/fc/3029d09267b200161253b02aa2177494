package nameservice;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.net.ServerSocket;

public class NameServiceThread extends Thread{
    public void run() {
    	try {
			ServerSocket serSock = new ServerSocket(8889);
		} catch (IOException e) {
			System.out.println("Could not create server socket - nameService!");
			e.printStackTrace();
		}
    	
		in = new BufferedReader(new InputStreamReader(serSock.getInputStream()));
		out = mySock.getOutputStream();	
    }
}
