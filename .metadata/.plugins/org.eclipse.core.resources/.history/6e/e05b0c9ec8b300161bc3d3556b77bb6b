package mware_lib;

import java.io.Serializable;

public class RawObject implements Serializable{
	String host;
	int port;
	Object obj;
	
	public RawObject(String host, int port, Object obj){
		this.host = host;
		this.port = port;
		this.obj = obj;
	}
	
	public RawObject(String string){
		value = string;
	}
	
	public String toString(){
		return value;
	}
	
}
